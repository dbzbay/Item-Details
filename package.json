{
  "name": "webpack",
  "version": "1.0.0",
  "description": "A dope webpack boilerplate",
  "main": "index.js",
  "scripts": {
    "watch": "rimraf dist && webpack --watch",
    "start": "webpack-dev-server  --open --color ",
    "build": "rimraf dist &&  webpack --progress --profile",
    "server": "npx nodemon server/index.js",
    "server-prod": "node server/index.js",
    "restart": "pm2 startOrRestart ecosystem.config.js",
    "deploy": "pm2 deploy ecosystem.config.js production"
  },
  "repository": {
    "type": "git",
    "url": "none yet"
  },
  "author": "James Dempsey",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongodb": "^3.2.7",
    "mongoose": "^5.6.3",
    "npm": "^6.10.0",
    "pm2": "^3.5.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-loadable": "^5.4.0",
    "react-router-dom": "^4.2.2",
    "run": "^1.4.0",
    "watch": "^1.0.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              ">1%",
              "last 3 versions"
            ]
          }
        }
      ],
      "stage-2",
      "latest",
      "react"
    ],
    "plugins": [
      "syntax-dynamic-import",
      "transform-class-properties"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "dotenv": "^8.0.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-express": "^1.10.1",
    "mini-css-extract-plugin": "^0.4.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "supertest": "^4.0.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.4"
  }
}
